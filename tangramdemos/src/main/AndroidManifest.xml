<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mapzen.tangramdemos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androidtangramdemos">
        <activity android:name=".DemoMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FeaturePickingActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".MapGesturesActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".MapMovementActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".MarkersActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".MultiMapActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".SceneUpdatesActivity" android:parentActivityName=".DemoMainActivity"/>
        <activity android:name=".SimpleMapActivity" android:parentActivityName=".DemoMainActivity"/>
    </application>

</manifest>